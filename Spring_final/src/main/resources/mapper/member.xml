<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.spring.model.member.MemberDAO">
	
	<insert id="join" parameterType="com.spring.model.member.MemberDTO">
		insert into Trv_User values (#{U_id}, #{U_pwd}, #{U_name}, #{U_email}, #{U_phone}, #{U_img}, #{U_oauth})	
	</insert>
	
	<select id="check" parameterType="string" resultType="com.spring.model.member.MemberDTO">
		select * from Trv_User where U_id = #{U_id}
	</select>

</mapper>